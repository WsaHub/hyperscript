{
  "name": "hyperscript.org",
  "description": "a small scripting language for the web",
  "keywords": [
    "scripting",
    "HTML"
  ],
  "version": "0.8.3",
  "homepage": "https://hyperscript.org/",
  "bugs": {
    "url": "https://github.com/bigskysoftware/_hyperscript/issues"
  },
  "license": "BSD 2-Clause",
  "files": [
    "LICENSE",
    "README.md",
    "dist/*.js"
  ],
  "main": "dist/_hyperscript_web.min.js",
  "exports": "./dist/_hyperscript_web.modern.js",
  "module": "./dist/_hyperscript_web.esm.js",
  "unpkg": "dist/_hyperscript.min.js",
  "bin": {
    "_hyperscript": "src/bin/node-hyperscript.js"
  },
  "scripts": {
    "test": "mocha-chrome test/index.html",
    "www": "node scripts/www.js",
    "dist": "npm run clear && npm run copy && npm run bundle-all",
    "clear": "rm -rf dist/*",
    "copy": "cp -r src/* dist/",
    "bundle-dev": "microbundle src/web/_hyperscript_w9y.js -o dist/_hyperscript_w9y.js --name=_hyperscript -f umd",
    "bundle-all": "microbundle src/web/* -o dist --name=_hyperscript -f modern,esm,umd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigskysoftware/_hyperscript.git"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "chai": "^4.2.0",
    "eleventy-plugin-sass": "^1.0.0",
    "fs-extra": "^9.0.0",
    "microbundle": "^0.13.3",
    "mocha": "^7.1.1",
    "mocha-chrome": "^2.2.0",
    "node-sass": "^5.0.0",
    "sass": "^1.32.8",
    "sinon": "^9.0.2"
  }
}
